#you_reach
    background-image: url("../img/about_back.jpg")
    background-repeat: no-repeat;
    background-position: center;
    background-attachment fixed
    background-size: cover
    padding-bottom: 50px
    .item
        border: 1px solid #555555
        border-radius: 5px;
        background-color: rgba(255, 255, 255, 0.051);
        width: 650px
        height: auto
        text-align: center
        padding: 0 20px 20px
        margin 0 auto 85px
        img
            margin: -55px auto 10px
        h5
            font-size: 24px;
            font-family: "OswaldRegular", Arial;
            color: #88d6e7
            line-height: 1.3; 
            margin-bottom: 20px
        p
            font-size: 18px;
            font-family: "OpenSansRegular", arial;
            color: rgba(255, 255, 255, 0.9);
            line-height: 1.375;
            text-align left
            margin-bottom: 20px